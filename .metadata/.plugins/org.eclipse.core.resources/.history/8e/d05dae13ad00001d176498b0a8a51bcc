import java.util.Scanner;

public class Test10 {

	public static void main(String[] args) {
		System.out.println("1부터 n까지의 총합 구하기");
		Scanner sc =  new Scanner(System.in);
		int num = sc.nextInt();
		
		
		
		int sum = 0;
		
		for (int i = 1; i <= num; i++) {
			if(i==-num&&i==0)continue;
			sum+=1;
			System.out.println("n값:" +num);
		}
		
		System.out.printf("1부터 n%d까지 총합:%d",num,sum);
	}	
}
